      </span><span class="s2">"ssn"</span><span class="p">:</span><span class="w"> </span><span class="s2">"One required*"</span><span class="p">,</span><span class="w">
      </span><span class="s2">"short_ssn"</span><span class="p">:</span><span class="w"> </span><span class="s2">"Optional"</span><span class="p">,</span><span class="w">
